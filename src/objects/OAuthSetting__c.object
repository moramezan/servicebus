<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <description>Holds the credentials needed to manage the Broker running user context. #774 formerly OAuth Setting</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>AccessToken__c</fullName>
        <description>Roaring and ready access token that can be used to invoke Salesforce APIs.</description>
        <externalId>false</externalId>
        <label>Access Token</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>InstanceUrl__c</fullName>
        <description>The protocol and host (https://eu2.salesforce.com) of the actual instance. We need to hang onto this because it cannot be part of the callback URL. While the OAuth response indeed contains this, we don&apos;t know it at the time of making the OAuth request.</description>
        <externalId>false</externalId>
        <inlineHelpText>What is the API protocol and host for this instance?</inlineHelpText>
        <label>Instance URL</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Url</type>
    </fields>
    <fields>
        <fullName>RefreshToken__c</fullName>
        <description>The access token should be refreshed periodically using this refresh token.</description>
        <externalId>false</externalId>
        <label>Refresh Token</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>ReturnUrl__c</fullName>
        <description>Used to redirect users back to some specific page after the configuration flow.</description>
        <externalId>false</externalId>
        <inlineHelpText>What retURL will the user be redirected to afterwards?</inlineHelpText>
        <label>Return URL</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Url</type>
    </fields>
    <fields>
        <fullName>RunningUserId__c</fullName>
        <defaultValue>$User.Id</defaultValue>
        <description>Which User with a System Administrator profile performed the post install configuration ?</description>
        <externalId>false</externalId>
        <inlineHelpText>Which User with a System Administrator profile performed the post install configuration ?</inlineHelpText>
        <label>Running User Id</label>
        <length>18</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>UnprefixedOrgId__c</fullName>
        <description>Without the 00D Key Prefix, so that the value does not get magically replaced when refreshing sandboxes etc.</description>
        <externalId>false</externalId>
        <inlineHelpText>What was the unprefixed Organization Id at configuration time?</inlineHelpText>
        <label>UnprefixedOrgId</label>
        <length>12</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Setting</label>
    <visibility>Protected</visibility>
</CustomObject>
