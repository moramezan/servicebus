<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <description>Controls which broker implementation to use and whether it should run at all.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Inhibit__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Set this TRUE to interrupt a haywire Broker during development.</description>
        <externalId>false</externalId>
        <label>Inhibit</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>IsPaused__c</fullName>
        <defaultValue>false</defaultValue>
        <description>When checked, this setting prevents new Messages from being processed. They will still enter the queue, but unresolved in a &apos;Paused&apos; status.</description>
        <externalId>false</externalId>
        <label>IsPaused</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>LastTimestamp__c</fullName>
        <description>A timestamp representing the last time that a heartbeat message was successfully enqueued to the broker</description>
        <externalId>false</externalId>
        <inlineHelpText>What was the last heartbeat timestamp generated?</inlineHelpText>
        <label>Last Timestamp</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>DateTime</type>
    </fields>
    <label>Broker Settings</label>
    <visibility>Protected</visibility>
</CustomObject>
